{
    "name": "encore/easy-fa",
    "description": "A easy way use icons for phper",
    "type": "library",
    "keywords": ["font awesome", "icon"],
    "homepage": "https://github.com/z-song/easy-fa",
    "license": "MIT",
    "authors": [
        {
            "name": "zsong",
            "email": "zosong@126.com"
        }
    ],
    "require": {
        "php": ">=5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "symfony/css-selector": "2.8.*|3.0.*",
        "symfony/dom-crawler": "2.8.*|3.0.*"
    },
    "autoload": {
        "psr-4": {
            "Encore\\EasyFontAwesome\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    }
}
